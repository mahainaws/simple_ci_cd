pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sleep 10
                echo 'Building..'
                
            }
        }
        stage('Test') {
            steps {
                sleep 7
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo sleep 4
                echo 'Deploying....'
            }
        }
    }
}
